        <script>
            function addMediaEmbedClassnames() {
                $('.vine-embed').parent().parent().parent().addClass('vine-container');
                $('.instagram-media').parent().parent().parent().addClass('instagram-container');
            }

            addMediaEmbedClassnames();

            $(document).foundation();
            
            //disqus
            {block:IfDisqusShortname}
            {block:PermalinkPage}
                var disqus_shortname = '{text:Disqus Shortname}'; 
                var disqus_url = '{Permalink}'; 
                (function() {
                    var dsq = document.createElement('script'); dsq.type = 'text/javascript'; dsq.async = true;
                    dsq.src = '//' + disqus_shortname + '.disqus.com/embed.js';
                    (document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(dsq);
                })();
            {/block:PermalinkPage}
            {/block:IfDisqusShortname}
            
            //pre loader
            window.addEventListener('DOMContentLoaded', function() {
                new QueryLoader2(document.querySelector("body"), {
                    barColor: "{color:Loading Screen Text Color}",
                    backgroundColor: "{color:Loading Screen Background Color}",
                    percentage: true,
                    barHeight: 1,
                    minimumTime: 200,
                    fadeOutTime: 1000
                });
            });

            //instagram Feed
            {block:IfInstagramID} 
            $("#instagram-feed").jqinstapics({
                "user_id": "{text:Instagram ID}",
                "access_token": "{text:Instagram Token}",
                "count": {text:Instagram Feed Count}
            });
            {/block:IfInstagramID}
            
            //infinite scroll
            (function () {
                var infiniteContainer = new InfiniteScroll( '.content-area', {
                    nextSelector: "#load-more a",
                    path: "#load-more a",
                    append: ".post",
                    status: '.page-load-status',
                    {block:IfNotInfiniteScroll}
                    button: '#load-more a',
                    scrollThreshold: false
                    {/block:IfNotInfiniteScroll}
                });

                infiniteContainer.on( 'append', function( response, path, items ) {
                    addMediaEmbedClassnames();

                    var $newElemsIDs = $(items).map(function () { return this.id; }).get();
                    
                    //get like status for posts
                    Tumblr.LikeButton.get_status_by_post_ids($newElemsIDs);

                    $(function(){
                        var $elems = $('.animateblock');
                        var winheight = $(window).height();
                        var fullheight = $(document).height();

                        $(window).scroll(function(){
                            animate_elems();
                        });

                        function animate_elems() {
                            wintop = $(window).scrollTop(); 
                            $elems.each(function(){
                                $elm = $(this);
                                if($elm.hasClass('animated')) { return true; }
                                topcoords = $elm.offset().top;
                                if(wintop > (topcoords - (winheight*1))) {
                                $elm.addClass('animated');
                                }
                            });
                        } 
                    });
                    
                    $(document).foundation('reveal', 'reflow');
                    
                    {block:IfStandardPhotosetLayout}
                    //responsive photosets
                    resizePhotosets();
                    {/block:IfStandardPhotosetLayout}
                });
            })();

            {block:IfStandardPhotosetLayout}
            //responsive photoset
            function resizePhotosets(){
                var parentWidth = document.body.querySelector('.html_photoset').offsetWidth; 
                var photosets = document.body.querySelectorAll('iframe.photoset');
                for(var i = 0; i < photosets.length; ++i){
                    var photoset = photosets[i];
                    photoset.width = parentWidth;
                }   
            }
            window.onload = resizePhotosets;
            window.onresize = resizePhotosets; 
            {/block:IfStandardPhotosetLayout}
            

            // Fullscreen handler
            function fullscreenHandler(e) {
                if( window.fullscreen == false || window.fullscreen == null ) {
                    window.fullscreen = true;
                } else {
                    window.fullscreen = false;
                }

                if( window.fullscreen == true ) {
                    $('body').addClass('fullscreen');
                } else {
                    $('body').removeClass('fullscreen');
                }
            }

            // Add event listeners
            document.addEventListener("fullscreenchange", fullscreenHandler, false);
            document.addEventListener("webkitfullscreenchange", fullscreenHandler, false);
            document.addEventListener("mozfullscreenchange", fullscreenHandler, false);

            // Add resize sensors to video wrapper and assign height to .post data-height
            new ResizeSensor($('.post-media.video'), function() {
                $('.post-media.video').each(function(index) {
                    var elemHeight = $(this).innerHeight()
                    $(this).closest('article.post').attr( 'data-height', elemHeight );
                });
            });
        </script>

        {block:IfNotHideTwitterFeed}{block:Twitter}
        <script type="text/javascript">
            function recent_tweets(data) {
                for (i=0; i < {text:Twitter Feed Count}; i++) {
                    document.getElementById("tweets").innerHTML =
                        document.getElementById("tweets").innerHTML +   
                        '<p><a href="//twitter.com/{TwitterUsername}/status/' +
                        (data[i].id_str ? data[i].id_str : data[i].id) +
                        '" " target="_blank">' + data[i].text +
                        '</a></p>';
                }
                document.getElementById("twitter").style.display = 'block';
            }
        </script>
        <script type="text/javascript" src="/tweets.js"></script>
        {/block:Twitter}{/block:IfNotHideTwitterFeed}
        
    </body>
    
</html> <conditions>
            <add input="{REQUEST_FILENAME}" matchType="IsFile" negate="True"/>
          </conditions>
